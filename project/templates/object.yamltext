types:
  - kind: Object
    name: World
    spec: WorldSpec
    status: WorldStatus
    primarykey: spec.name
    apimethods:
      - apimethod: GET
      - apimethod: PUT
      - apimethod: POST
      - apimethod: DELETE
  - kind: Struct
    name: WorldSpec
    properties:
      - name: name
        type: string
  - kind: Struct
    name: WorldStatus
    properties:
      - name: description
        type: string